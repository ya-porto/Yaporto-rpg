{
    "compilerOptions": {
        "jsx": "react",
        "target": "esnext",
        "module": "commonjs",
        "lib": ["DOM", "ESNext"],
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "node_modules"
    ]
}